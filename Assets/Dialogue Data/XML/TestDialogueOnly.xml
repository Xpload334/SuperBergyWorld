<?xml version="1.0" encoding="UTF-8"?>
<dialogueObjects>
    <!--
    <dialogueObject>
        <dialogueName> DialogueName </dialogueName> (NPCs will search for this name)
        (examples: W1Landing, W1FirstBattle_1, W1FirstBattle_2)
        <line>
            <typerName>CharacterTyper</typerName> (refer to TyperStore.xml) 
            <text>Text to say!</text>
            <ShouldAutoSkip>true</ShouldAutoSkip> (Optional, default false)
            <autoSkipSeconds>1</autoSkipSeconds> (Optional, default 0)
        </line>
    </dialogueObject>
    -->




    <dialogueObject><!-- Opening and closing tags for each dialogue object -->
        <dialogueName>Test1</dialogueName><!-- Name of the dialogue object -->
        <line><!-- Use a new line tag for each line of dialogue -->
            <typerName>TobiasBasic</typerName><!-- Specify the typer of who is speaking -->
            <text>Example text!</text><!-- Here is where the text goes -->
            <autoSkipSeconds>1</autoSkipSeconds><!-- (Optional) Take away player input, and show how long to wait before continuing to next line -->
        </line>
        <line><!-- Next line in the object should play once the previous is done -->
            <typerName>XploadBasic</typerName>
            <text>Example text on another line!</text>
        </line>
    </dialogueObject>

    <dialogueObject><!-- A different dialogue object -->
        <dialogueName>Test2</dialogueName>
        <line>
            <typerName>XploadBasic</typerName>
            <text>XBOX LIVE !!</text>
        </line>
    </dialogueObject>

    <dialogueObject>
        <dialogueName>TestNeutral</dialogueName>
        <line>
            <typerName>Neutral</typerName>
            <text>Test with no face</text>
        </line>
    </dialogueObject>
</dialogueObjects>